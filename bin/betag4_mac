#!/bin/bash
source /home/whit/.bashrc
macfile=${1}

#printenv
#echo $LD_LIBRARY_PATH  | sed 's/:/\n/g'
cd $HOME/sane08/BETAG4/run; 
pwd
runnum=`cat run.txt`
runnum=`expr $runnum + 1`
mkdir log/$runnum

cat $macfile >> log/$runnum/betag4.mac

BETA $macfile 1>> log/$runnum/betag4.out 2>> log/$runnum/betag4.err

#wait


